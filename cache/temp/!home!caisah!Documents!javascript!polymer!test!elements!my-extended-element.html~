<link rel="import" href="../bower_components/polymer/polymer.html">

<polymer-element name="my-extended-element"
                 extends="button"
                 on-click="increment">

    <template>
        Increment:
    </template>

    <script>
      Polymer({
          counter: 0,
          increment: function(e, detail, sender) {
              this.counter++;
          }
      })
    </script>
</polymer-element>
